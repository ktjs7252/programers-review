#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#define max(a,b) (a)>(b) ? (a):(b)
#define min(a,b) (a)<(b) ? (a):(b)

long long solution(int w, int h) {
    long long answer = 1;
    long long i,gcd=0;
    long long k=min(w,h);
    long long sum=(long long)w*(long long)h;
    if(w==h)
    {
        answer=max(w,h);
    }
    else
    {
        for(i=k;i>=1;i--)
        {
            if(w%i==0 && h%i==0)
            {
                gcd=i;
                break;
            }
        }
    }
    answer=sum-((long long)w+(long long)h-gcd);
    return answer;
}
